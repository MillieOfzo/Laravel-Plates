{
    "name": "millieofzo/laravel-plates",
    "description": "Bringing powerful native PHP templating system Plates to Laravel",
    "type": "library",
    "readme": "README.md",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Roelof",
            "email": "r_j_vangolen@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5",
        "illuminate/view": "~5",
        "league/plates": "~3.3"
    },
    "autoload": {
        "psr-4": {
            "MillieOfzo\\Plates\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MillieOfzo\\Plates\\PlatesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
